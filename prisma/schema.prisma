generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "mongodb"
  url      = env("DATABASE_URL")
}


model User {
  id    String @id @default(auto()) @map("_id") @db.ObjectId
  email String @unique
  username String @unique
  password String @default("")
  google_id String @unique
  plaid_token String @default("")
  total_transcations_30days Json 
  uncategorized_transcation_30days Json
  total_income_30days Float 
  need_transcations_30days Json
  want_transcations_30days Json
  investment_transcations_30days Json
}